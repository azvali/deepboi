{
  "name": "deepboi",
  "displayName": "Deepboi",
  "description": "A VS Code extension for DeepSeek AI chatbot.",
  "version": "0.0.1",
  "publisher": "yousefm2315", 
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:deepboi.start"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "deepboi.start",
        "title": "Deepest of Bois",
        "category": "DeepBoi"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "start": "code --extensionDevelopmentPath=.",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "ollama": "^0.5.12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/deepboi"
  },
  "license": "MIT"
}
